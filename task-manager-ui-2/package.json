{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test-mocha": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test-ava": "ava \"src/**/*.test.js\" --verbose",
    "test-mocha:watch": "npm run test -- --watch",
    "coverage": "nyc mocha",
    "test": "mocha",
    "test:watch": "mocha --watch --reporter min"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "jquery": "^2.2.1",
    "jsdom": "11.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-preset-stage-1": "^6.1.18",
    "babel-register": "^6.26.0",
    "jsdom-global": "^3.0.2",
    "lodash": "^3.10.1",
    "nyc": "^12.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "4.3.0",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.0.4",
    "redux-form": "^6.6.3",
    "redux-promise": "^0.5.3"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js*",
      "test/**"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
